<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheet/style.css">
    <script src="https://cdn.tailwindcss.com"></script>
  </head>
  <body>
    <div class="w-full h-full bg-black-900 p-10 text-white">
        <div class="text-blue-500">
            <a href="/">back to home</a>
        </div>
      <h1 class="text-3xl tracking-tighter mb-3"> ALL user </h1>
    <div class="users flex gap-4 flex-wrap">
        <% users.forEach(function(user){ %>
             <div class="user w-72  p-4 bg-zinc-800 rounded-lg">
           <div class="w-full h-52 rounded-lg bg-zinc-700 overflow-hidden">
            <img class="w-full h-full object-cover object-top" src="<%= user.image %>">
           </div>
            <h3 class="text-xl tracking-tighter"><%= user.name %></h3>
            <h5 class="text-zinc-500"><%= user.email %></h5>
            <div class="flex gap-3 mt-10">
                <a class="text-blue-400" href="">edit user</a>
                <a class="text-red-500" href="">delet user</a>
            </div>
        </div>
        <% }); %>
    </div>
    </div>
  </body>
</html>